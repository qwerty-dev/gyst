.intro-header
    .container
        .row
            .col-lg-12
%div(class ="users-wrapper")
    - @users.each do |user|
        %h4
            %a{:href => "/user/#{user.id}"} #{user.username}

%div
    = @debug
    - if session[:evernote_token]
        - @content.each do | note | 
            = note